@article{maia2020d,
	title = {bibliometric analysis of scientific jurimetric articles published in brazil},
	author = {maia, organization},
	journal = {rdbci revista digital de library and information science},
	abstract = {abstract a judicial slowness has been a topic of debate in the brazilian academic community. among the solutions discussed, jurimetrics has gained ground when searching for quantitative standards in judicial decisions. however, despite the relevance of this methodology, in brazil, little research has been conducted towards its dissemination. therefore, the present study employs bibliometric techniques on national scientific production, articles, related to the term "jurimetrics", from 2002 to 2019, made available in the google scholar search tool, presenting an informative panel on the characteristics found in these publications. the results show that in the period researched, the number of publications on the topic showed an average annual growth of 18.92\%; the collaboration index indicates that there are, on average, 2.32 authors per article, some of which stand out due to the number of publications, among them, daniel fn menezes (four) and filipe j. zabala (three) and, in turn, , the most cited authors are lee loevinger (eleven citations), filipe j. zabala and fabiano f. silveira (both with six citations); the magazine that has published the most on the topic is the journal of the faculty of law of the federal university of minas gerais, with a single magazine publishing four articles, seven magazines two articles and 66 magazines a single article and, finally; as for the keywords associated with the term, the following words stand out: "adoption", "civil process", "invalid act", "illicit act", "partial dissolution", and "successive incidence", suggesting civil law as an area of law more aligned with the use of jurimetrics. keywords legal scientific production. empirical legal studies. bibliometric indicators.}
}
