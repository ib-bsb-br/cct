@article{colombo2017,
	title = {challenges when using jurimetrics in brazil--a survey of courts},
	author = {armonas colombo, bruna and colombo, bruna armonas and buck, pedro and miana bezerra, vinicius and bezerra, vinicius miana},
	year = 2017,
	month = oct,
	journal = {future internet},
	publisher = {mdpi ag},
	volume = 9,
	number = 4,
	pages = 68,
	doi = {10.3390/fi9040068},
	issn = {1999-5903},
	url = {https://www.mdpi.com/1999-5903/9/4/68/pdf?version=1508922455},
	abstract = {jurimetrics is the application of quantitative methods, usually statistics, to law. an important step to implementing a jurimetric analysis is to extract raw data from courts and organize that data in a way that can be processed. most of the raw data is unstructured and written in natural language, which stands as a challenge to computer science experts. as it requires expertise in law, statistics, and computer science, jurimetrics is a multidisciplinary field. when trying to implement a jurimetric system in brazil, additional challenges were identified due to the heterogeneity of the different court systems, the lack of standards, and how the open data laws in brazil are interpreted and implemented. in this article, we present a survey of brazilian courts in terms of readiness to implement a jurimetric system. analyzing a sample of data, we have found, in light of brazil's open data regulation, privacy issues and technical issues. finally, we propose a roadmap that encompasses both technology and public policy to meet those challenges.},
	file = {:c\:/users/mseix/documents/tcc/references/armonascolombo2017.pdf:pdf},
	keywords = {jurimetrics; legal informatics; public policy; open data; computational law; e-government},
	issue = 4
}
