@article{Maia2020c,
	title = {Análise bibliométrica dos artigos científicos de jurimetria publicados no Brasil},
	author = {Maia, Marcos and Bezerra, Cícero Aparecido},
	abstract = {
RESUMO A morosidade judicial tem sido tema de debates na comunidade
acad\^{e}mica brasileira. Entre as solu\c{c}\~{o}es discutidas, a
jurimetria tem ganhado espa\c{c}o ao buscar por padr\~{o}es quantitativos
nas decis\~{o}es judiciais. Por\'{e}m, em que pese a relev\^{a}ncia desta
metodologia, no Brasil, poucas pesquisas t\^{e}m sido conduzidas no sentido
de sua dissemina\c{c}\~{a}o. Assim sendo, o presente estudo emprega
t\'{e}cnicas de bibliometria sobre a produ\c{c}\~{a}o cient\'{\i}fica
nacional, artigos, relacionada ao termo "jurimetria", de 2002 at\'{e} 2019,
disponibilizada na ferramenta de pesquisa do Google Acad\^{e}mico,
apresentando um painel informativo sobre as caracter\'{\i}sticas
encontradas nestas publica\c{c}\~{o}es. Os resultados mostram que no
per\'{\i}odo pesquisado, o n\'{u}mero de publica\c{c}\~{o}es sobre o tema
apresentou um crescimento m\'{e}dio anual de 18,92\%; o \'{\i}ndice de
colabora\c{c}\~{a}o indica que existem, em m\'{e}dia, 2,32 autores por
artigo, sendo que alguns se destacam pelo n\'{u}mero de
publica\c{c}\~{o}es, entre eles, Daniel F. N. Menezes (quatro) e Filipe J.
Zabala (tr\^{e}s) e, por sua vez, os autores mais citados s\~{a}o Lee
Loevinger (onze cita\c{c}\~{o}es), Filipe J. Zabala e Fabiano F. Silveira
(ambos com seis cita\c{c}\~{o}es); j\'{a} a revista que mais tem publicado
sobre o tema \'{e} a Revista da Faculdade de Direito da Universidade
Federal de Minas Gerais, sendo que uma \'{u}nica revista publicou quatro
artigos, sete revistas dois artigos e 66 revistas um \'{u}nico artigo e,
finalmente; quanto \`{a}s palavras-chaves associadas ao termo, destacam-se
as palavras: "ado\c{c}\~{a}o", "processo civil", "ato inv\'{a}lido", "ato
il\'{\i}cito", "dissolu\c{c}\~{a}o parcial", e, "incid\^{e}ncia sucessiva",
sugerindo o Direito Civil como uma \'{a}rea do direito mais alinhada ao uso
da jurimetria. PALAVRAS-CHAVE Produ\c{c}\~{a}o cient\'{\i}fica
jur\'{\i}dica. Estudos jur\'{\i}dicos emp\'{\i}ricos. Indicadores
bibliom\'{e}tricos.
},
	doi = {10.20396/rdbci.v18i0.8658889},
	file = {:/home/mario/Documents/UFJ/TCC/02cap/REFERENCES/bibliometria.pdf:PDF},
	issn = {1678-765X},
	journal = {RDBCI Revista Digital de Biblioteconomia e Ciência da Informação},
	keywords = {
Legal scientific production, Empirical legal studies, Bibliometric
indicators JITA: BB, Bibliometric methods
},
	month = {6},
	pages = {e020018},
	publisher = {University of Campinas},
	volume = {18},
	year = {2020},
}

