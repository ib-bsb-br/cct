@inproceedings{de2020,
	title = {Revista},
	author = {
A jurimetria e sua aplica\c{c}\~{a}o nos tribunais de and contas:
an\'{a}lise de estudo sobre o Tribunal and de
},
	abstract = {
The object of this work is the application of Jurimetrics on the Courts of
Accounts. Jurimetrics is the use of quantitative methods in law. The main
purpose of the study is to demonstrate that the use of Jurimetrics can lead
the Courts of Accounts to improve their performance. The technique used in
the work was bibliographic research. The concept of Jurimetrics is
presented, as well as its characteristics and advantages. The following are
cases of successful application of Jurimetrics in Brazil. The bibliographic
research resulted in a single study, which applies statistical techniques
to data from TCU jurisprudence. From the analysis of the result it is
concluded that the application of Jurimetrics by the Courts of Accounts
will probably lead to an improvement in their performance, which shows that
the initial hypothesis is true. It concludes by recommending that the
Courts of Accounts apply the Jurimetrics.
},
	date = {2020},
	file = {
:/home/mario/Documents/UFJ/TCC/02cap/REFERENCES/Dialnet-AJurimetriaESuaAplicacaoNosTribunaisDeContas-7671524.pdf:PDF
},
	issn = {2237-6917},
	keywords = {
Jurimetrics, Courts of Accounts, Quantitative Methods, Federal Court of
Accounts, TCU
},
	month = {8},
	publisher = {EDUFU - Editora da Universidade Federal de Uberlandia},
	year = {2016},
}

